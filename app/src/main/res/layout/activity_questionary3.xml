<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_bg"
    tools:context=".Questionary3">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="290dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="4dp"
        android:layout_marginTop="133dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="105dp"
        android:text="Was It negative?"
        android:textAlignment="center"
        android:textColor="#4DFFFFFF"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="357dp"
        android:layout_height="494dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="28dp"
        android:layout_marginTop="175dp"
        android:layout_marginEnd="26dp"
        android:layout_marginBottom="62dp"
        app:srcCompat="@drawable/layout_bg" />

    <TextView
            android:id="@+id/textView4"
            android:layout_width="274dp"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/imageView7"
            android:layout_alignTop="@+id/imageView7"
            android:layout_alignEnd="@+id/imageView7"
            android:layout_alignBottom="@+id/imageView7"
            android:layout_marginStart="43dp"
            android:layout_marginTop="145dp"
            android:layout_marginEnd="40dp"
            android:layout_marginBottom="117dp"
            android:fontFamily="@font/rubik"
            android:text="We've saved your geolocation and symptoms."
            android:textAlignment="center"
            android:textColor="#000000"
            android:textSize="30sp"
            android:textStyle="bold|italic" />

    <TextView
            android:id="@+id/textView8"
            android:layout_width="290dp"
            android:layout_height="60dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="5dp"
            android:layout_marginTop="47dp"
            android:layout_marginEnd="115dp"
            android:layout_marginBottom="105dp"
            android:fontFamily="@font/rubik"
            android:text="Have you been tested for COVID-19 infection?"
            android:textAlignment="center"
            android:textColor="#26FFFFFF"
            android:textSize="18sp" />

    <TextView
            android:id="@+id/textView"
            android:layout_width="98dp"
            android:layout_height="112dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="267dp"
            android:layout_marginTop="52dp"
            android:layout_marginEnd="45dp"
            android:fontFamily="@font/rubik"
            android:text="YES"
            android:textAlignment="center"
            android:textColor="#26FFFFFF"
            android:textSize="24sp" />

    <TextView
            android:id="@+id/textView10"
            android:layout_width="98dp"
            android:layout_height="112dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="265dp"
            android:layout_marginTop="126dp"
            android:layout_marginEnd="47dp"
            android:fontFamily="@font/rubik"
            android:text="NO"
            android:textAlignment="center"
            android:textColor="#4DFFFFFF"
            android:textSize="24sp" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="197dp"
        android:layout_height="149dp"
        android:layout_alignStart="@+id/imageView7"
        android:layout_alignTop="@+id/imageView7"
        android:layout_alignEnd="@+id/imageView7"
        android:layout_alignBottom="@+id/imageView7"
        android:layout_marginStart="93dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="67dp"
        android:layout_marginBottom="318dp"
        app:srcCompat="@drawable/ic_group_5" />

    <Button
            android:id="@+id/button"
            style="?android:attr/borderlessButtonStyle"
            android:layout_width="wrap_content"
            android:layout_height="108dp"
            android:layout_alignStart="@+id/imageView7"
            android:layout_alignTop="@+id/imageView7"
            android:layout_alignEnd="@+id/imageView7"
            android:layout_alignBottom="@+id/imageView7"
            android:layout_marginStart="42dp"
            android:layout_marginTop="362dp"
            android:layout_marginEnd="37dp"
            android:layout_marginBottom="24dp"
            android:background="#FFFFFF"
            android:fontFamily="@font/rubik"
            android:onClick="onClick"
            android:text="Go to dashboard"
            android:textColor="#00BCD4"
            tools:text="Go to dashboard" />

</RelativeLayout>